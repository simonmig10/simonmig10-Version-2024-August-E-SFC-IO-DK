  (c_120_dom_9 + c_130_dom_9 + c_110_dom_9 + c_140_dom_9 + c_160_dom_9 + c_180_dom_9 + c_oth_dom_9) / pout_9 +
  (c_120_im_1 + c_130_im_1 + c_110_im_1 + c_140_im_1 + c_160_im_1 + c_180_im_1 + c_oth_im_1) / pim_1 +
  (c_120_im_2 + c_130_im_2 + c_110_im_2 + c_140_im_2 + c_160_im_2 + c_180_im_2 + c_oth_im_2) / pim_2 +
  (c_120_im_3 + c_130_im_3 + c_110_im_3 + c_140_im_3 + c_160_im_3 + c_180_im_3 + c_oth_im_3) / pim_3 +
  (c_120_im_4 + c_130_im_4 + c_110_im_4 + c_140_im_4 + c_160_im_4 + c_180_im_4 + c_oth_im_4) / pim_4 +
  (c_120_im_5 + c_130_im_5 + c_110_im_5 + c_140_im_5 + c_160_im_5 + c_180_im_5 + c_oth_im_5) / pim_5 +
  (c_120_im_6 + c_130_im_6 + c_110_im_6 + c_140_im_6 + c_160_im_6 + c_180_im_6 + c_oth_im_6) / pim_6 +
  (c_120_im_7 + c_130_im_7 + c_110_im_7 + c_140_im_7 + c_160_im_7 + c_180_im_7 + c_oth_im_7) / pim_7 +
  (c_120_im_8 + c_130_im_8 + c_110_im_8 + c_140_im_8 + c_160_im_8 + c_180_im_8 + c_oth_im_8) / pim_8 +
