COMMENT> Total net production taxes paid by firms
IDENTITY> n_prod_tax_tot
EQ> n_prod_tax_tot =  ct_tot + vat_tot + im_d_tot


COMMENT> Total import taxes paid
IDENTITY> im_t_p
EQ> im_t_p = im_d_tot + oth_im_tax_p

COMMENT> Net import taxes receieved by Denmark
IDENTITY> im_t
EQ> im_t = im_t_r - im_t_p


COMMENT> income on capital related to interest
IDENTITY> nint_g
EQ> nint_g =  (TSLAG(idep,1)*(TSLAG(ndepo_g,1)+TSLAG(ndir_g,1) + TSLAG(ncred_g,1)) + TSLAG(ibd,1)*TSLAG(nsec_g,1) + TSLAG(iloan,1)*TSLAG(nloa_g,1)) + nint_g_adj

COMMENT> income on capital related to dividends
IDENTITY> ndiv_g
EQ> ndiv_g =  (TSLAG(divd,1)*(TSLAG(neq_g,1))) + ndiv_g_adj

COMMENT> income on capital related to dividends
IDENTITY> noir_g
EQ> noir_g = TSLAG(insu_rate,1)*(TSLAG(ninsu_g,1)) + noir_g_adj



COMMENT> Disposable income Government
IDENTITY> yd_g
EQ> yd_g =  n_prod_tax_tot + opt_tot + b2_g_r  + im_t + nint_g + ndiv_g + noir_g + re_fdi_r_g + i_tax_g + scon_r_g - sben_p_g + oct_g 


COMMENT> Taxes received by government
IDENTITY> i_tax_g
EQ> i_tax_g = i_tax_nfc + i_tax_fc + i_tax_h + ni_tax_row



COMMENT> 
IDENTITY> s_g
EQ> s_g = yd_g - g

COMMENT> 
IDENTITY> K_g
EQ> K_g = TSLAG(K_g,1) + i_g - dep_g + K_g_rev 

COMMENT> 
IDENTITY> K_g_k
EQ> K_g_k = TSLAG(K_g_k,1) + (i_g/pin) - DeltaK_g_k * TSLAG(K_g_k,1)

COMMENT> Sectoral balance
IDENTITY> nl_g
EQ> nl_g = s_g - i_g - invent_g - np_g - ct_g


COMMENT> ######################################## Financial side  ##################################


COMMENT> Financial Sectoral balance G (new) disaggregated
IDENTITY> fnl_g
EQ> fnl_g = ngold_g_tr  + ndepo_g_tr   + nsec_g_tr  + nloa_g_tr  + neq_g_tr   + ninsu_g_tr  + ndir_g_tr  + ncred_g_tr  



COMMENT>#################################### Assets vs liabilities ##########################################

COMMENT> Gold assets FA.1
IDENTITY> ngold_g
EQ> ngold_g = TSLAG(ngold_g,1) + ngold_g_tr + ngold_g_rv


COMMENT> deposits assets FA.2
IDENTITY> ndepo_g
EQ> ndepo_g = TSLAG(ndepo_g,1) + ndepo_g_tr + ndepo_g_rv


COMMENT> Securities assets FA.3
IDENTITY> nsec_g
EQ> nsec_g = TSLAG(nsec_g,1) + nsec_g_tr + nsec_g_rv

COMMENT> Securities FL.3 - sec_g_tr residual
IDENTITY> nsec_g_tr
EQ> nsec_g_tr = nl_g + nl_g_adj  - ngold_g_tr - ndepo_g_tr - nloa_g_tr - neq_g_tr - ninsu_g_tr - ndir_g_tr - ncred_g_tr


COMMENT> Loans assets FA.4 
IDENTITY> nloa_g
EQ> nloa_g = TSLAG(nloa_g,1) + nloa_g_tr + nloa_g_rv


COMMENT> Equities assets FA.5
IDENTITY> neq_g
EQ> neq_g = TSLAG(neq_g,1) + neq_g_tr + neq_g_rv


COMMENT> insurance NFC assets FA.6
IDENTITY> ninsu_g
EQ> ninsu_g = TSLAG(ninsu_g,1) + ninsu_g_tr + ninsu_g_rv


COMMENT> Financial derivatives NFC assets FA.7
IDENTITY> ndir_g
EQ> ndir_g = TSLAG(ndir_g,1) + ndir_g_tr + ndir_g_rv


COMMENT> Trade credits  NFC assets FA.8
IDENTITY> ncred_g
EQ> ncred_g = TSLAG(ncred_g,1) + ncred_g_tr + ncred_g_rv


COMMENT>  
IDENTITY> fnw_g
EQ> fnw_g = ngold_g  + ndepo_g   + nsec_g  + nloa_g  + neq_g   + ninsu_g  + ndir_g  + ncred_g 

COMMENT>  
IDENTITY> fnw_gk
EQ> fnw_gk = fnw_g/ppcon


COMMENT> ######################################## Rest of the world ########################################


COMMENT> ######################################## Real side



COMMENT> income on capital related to interest
IDENTITY> nint_row
EQ> nint_row =  (TSLAG(idep,1)*(TSLAG(ndepo_row,1)+TSLAG(ndir_row,1) + TSLAG(ncred_row,1)) + TSLAG(ibd,1)*TSLAG(nsec_row,1) + TSLAG(iloan,1)*TSLAG(nloa_row,1)) + nint_row_adj

COMMENT> income on capital related to dividends
IDENTITY> ndiv_row
EQ> ndiv_row =  (TSLAG(divd,1)*(TSLAG(neq_row,1))) + ndiv_row_adj

COMMENT> income on capital related to dividends
IDENTITY> noir_row
EQ> noir_row = TSLAG(insu_rate,1)*(TSLAG(ninsu_row,1)) + noir_row_adj

COMMENT> Alt ser ud til at passe 
IDENTITY> nl_row
EQ> nl_row = im - x + (w_row) - im_t +  nint_row + ndiv_row + noir_row + (re_fdi_r_row - re_fdi_p_row) - (ni_tax_row) - nscon_row + nsben_row + oct_row - ct_row - np_row 



COMMENT> ######################## Financial side ####################################

COMMENT> Financial Sectoral balance (Row) fnl_row = iba_row_tr + eq_row_a_tr - eq_row_l_tr + sec_row_tr + l_row_tr + ins_row_tr



COMMENT> Financial Sectoral balance ROW (new) disaggregated
IDENTITY> fnl_row
EQ> fnl_row = ngold_row_tr  + ndepo_row_tr   + nsec_row_tr  + nloa_row_tr + neq_row_tr   + ninsu_row_tr  + ndir_row_tr  + ncred_row_tr 



COMMENT> Gold assets FA.1
IDENTITY> ngold_row
EQ> ngold_row = TSLAG(ngold_row,1) + ngold_row_tr + ngold_row_rv


COMMENT> deposits assets FA.2
IDENTITY> ndepo_row
EQ> ndepo_row = TSLAG(ndepo_row,1) + ndepo_row_tr + ndepo_row_rv



COMMENT> deposits FL.2 - depo_row_tr residual
IDENTITY> ndepo_row_tr
EQ> ndepo_row_tr = nl_row + nl_row_adj - ngold_row_tr - nsec_row_tr - nloa_row_tr - neq_row_tr - ninsu_row_tr - ndir_row_tr - ncred_row_tr


COMMENT> Securities assets FA.3
IDENTITY> nsec_row
EQ> nsec_row = TSLAG(nsec_row,1) + nsec_row_tr + nsec_row_rv

COMMENT> 
IDENTITY> check_sec
EQ> check_sec = nsec_h + nsec_fc  + nsec_nfc + nsec_g + nsec_row


COMMENT> Loans assets FA.4 
IDENTITY> nloa_row
EQ> nloa_row = TSLAG(nloa_row,1) + nloa_row_tr + nloa_row_rv


COMMENT> Equities assets FA.5
IDENTITY> neq_row
EQ> neq_row = TSLAG(neq_row,1) + neq_row_tr + neq_row_rv


COMMENT> insurance NFC assets FA.6
IDENTITY> ninsu_row
EQ> ninsu_row = TSLAG(ninsu_row,1) + ninsu_row_tr + ninsu_row_rv


COMMENT> Financial derivatives NFC assets FA.7
IDENTITY> ndir_row
EQ> ndir_row = TSLAG(ndir_row,1) + ndir_row_tr + ndir_row_rv


COMMENT> Trade credits  NFC assets FA.8
IDENTITY> ncred_row
EQ> ncred_row = TSLAG(ncred_row,1) + ncred_row_tr + ncred_row_rv


COMMENT> Trade credits FL.8 - cred_row_tr is used as a residual
IDENTITY> ncred_row_tr
EQ> ncred_row_tr = - (ncred_nfc_tr + ncred_fc_tr + ncred_g_tr + ncred_h_tr)



COMMENT> 
IDENTITY> fnw_row
EQ> fnw_row = ngold_row  + ndepo_row   + nsec_row  + nloa_row  + neq_row   + ninsu_row  + ndir_row  + ncred_row 

COMMENT> 
IDENTITY> fnw_rowk
EQ> fnw_rowk = fnw_row/ppcon




COMMENT> ######################################## CONSISTENCY CHEKCS & variables for plots ########################################

COMMENT> 
IDENTITY> check_nl
EQ> check_nl= (nl_h + nl_fc + nl_nfc + nl_g + nl_row)

COMMENT> 
IDENTITY> check_np
EQ> check_np = np_row + np_h + np_fc + np_nfc + np_g

COMMENT> 
IDENTITY> check_ctr
EQ> check_ctr = ct_row + ct_h + ct_fc + ct_nfc + ct_g

COMMENT> 
IDENTITY> check_invest
EQ> check_invest = i - i_fc - i_nfc - i_g - i_h

COMMENT> 
IDENTITY> check_tax
EQ> check_tax = i_tax_g - i_tax_nfc - i_tax_fc - i_tax_h - ni_tax_row

COMMENT> Net exports
IDENTITY> nxk
EQ> nxk = xk-imk



END